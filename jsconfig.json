{
  "compilerOptions": {
    "target": "ES5",
    "baseUrl": "./",
    "types": ["node"],
    "paths": {
      "@app/*": ["app/*"],
      "@components/*": ["components/*"],
      "@hooks/*": ["hooks/*"],
      "@public/*": ["public/*"],
      "@styles/*": ["styles/*"],
      "@lib/*": ["lib/*"],
      "@ui/*": ["ui/*"],
      "@constants/*": ["constants/*"],
      "@utils/*": ["utils/*"]
    },
    "include": ["next-env.d.ts", "**/*.js", ".next/types/**/*.ts"],
    "exclude": ["node_modules"]
  }
}
